{
    "name": "erashdan/laravelfastly",
    "description": "A Laravel wrapper for Fastly SDK",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Laravel", "Php", "Fastly"
    ],
    "require": {
        "php": ">=7.1.3",
        "fastly/fastly": "^0.3.0|dev-master",
        "illuminate/support":
"~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4.2|~3.5.0|~3.6.0|~3.7.0|~3.8.0|^4.0",
        "phpunit/phpunit": "^5.7|6.2|^7.0"
    },
    "authors": [
        {
            "name": "Emad Rashdan",
            "email": "emadiga@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Erashdan\\LaravelFastly\\FastlyServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Erashdan\\LaravelFastly\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Erashdan\\LaravelFastly\\Test\\": "test"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
